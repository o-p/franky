{
  "name": "loki",
  "version": "1.0.0",
  "description": "Lots Of Known Issues",
  "main": "src/index.js",
  "scripts": {
    "build": "yarn build:home && yarn build:full && yarn build:rect && yarn build:quiz && yarn build:welcome:full && yarn build:welcome:rect && yarn build:welcome:quiz",
    "build:home": "node_modules/.bin/webpack --dist \"dist/home\" src/home.js && node scripts/build.js --path \"dist/home\"",
    "build:full": "node_modules/.bin/webpack --dist \"dist/full\" src/full.js && node scripts/build.js --path \"dist/full\"",
    "build:rect": "node_modules/.bin/webpack --dist \"dist/rect\" src/rect.js && node scripts/build.js --path \"dist/rect\"",
    "build:quiz": "node_modules/.bin/webpack --dist \"dist/quiz\" src/quiz.js && node scripts/build.js --path \"dist/quiz/\"",
    "build:welcome:full": "node_modules/.bin/webpack --dist \"dist/full/preview\" --template \"src/template-welcome.html\" ./src/welcome-full.js && node scripts/build.js --path \"dist/full/preview\"",
    "build:welcome:rect": "node_modules/.bin/webpack --dist \"dist/rect/preview\" --template \"src/template-welcome.html\" ./src/welcome-rect.js && node scripts/build.js --path \"dist/rect/preview\"",
    "build:welcome:quiz": "node_modules/.bin/webpack --dist \"dist/quiz/preview\" --template \"src/template-custom.html\" ./src/welcome-quiz.js && node scripts/build.js --path \"dist/quiz/preview\"",
    "dev": "npm run dev:home && npm run dev:full && npm run dev:rect && npm run dev:welcome:full && npm run dev:welcome:rect",
    "dev:home": "node_modules/.bin/webpack --dev --dist \"dist/home\" src/home.js",
    "dev:full": "node_modules/.bin/webpack --dev --dist \"dist/full\" src/full.js",
    "dev:rect": "node_modules/.bin/webpack --dev --dist \"dist/rect\" src/rect.js",
    "dev:quiz": "node_modules/.bin/webpack --dev --dist \"dist/quiz\" src/quiz.js",
    "dev:welcome:full": "node_modules/.bin/webpack --dev --dist \"dist/full/preview\" --template \"src/template-welcome.html\" src/welcome-full.js",
    "dev:welcome:rect": "node_modules/.bin/webpack --dev --dist \"dist/rect/preview\" --template \"src/template-welcome.html\" src/welcome-rect.js",
    "dev:welcome:quiz": "node_modules/.bin/webpack --dev --dist \"dist/quiz/preview\" --template \"src/template-custom.html\" src/welcome-quiz.js",
    "watch:home": "node_modules/.bin/webpack --dev --watch ./src/home.js --dist \"dist/home\" src/home.js",
    "watch:full": "node_modules/.bin/webpack --dev --watch ./src/full.js --dist \"dist/full\" src/full.js",
    "watch:rect": "node_modules/.bin/webpack --dev --watch ./src/rect.js --dist \"dist/rect\" src/rect.js",
    "watch:quiz": "node_modules/.bin/webpack --dev --watch --dist \"dist/quiz\" ./src/quiz.js",
    "watch:welcome:full": "node_modules/.bin/webpack --dev --watch --dist \"dist/full/preview\" --template \"src/template-welcome.html\" src/welcome-full.js",
    "watch:welcome:rect": "node_modules/.bin/webpack --dev --watch --dist \"dist/rect/preview\" --template \"src/template-welcome.html\" src/welcome-rect.js",
    "watch:welcome:quiz": "node_modules/.bin/webpack --dev --watch --dist \"dist/quiz/preview\" --template \"src/template-custom.html\" src/welcome-quiz.js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.kkcorp:AST/loki.git"
  },
  "keywords": [
    "loki"
  ],
  "author": "Chris Chu <chrischu@kkbox.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.5.0",
    "css-loader": "0.23.1",
    "debug": "2.6.8",
    "eslint": "2.9.0",
    "eslint-config-airbnb": "9.0.1",
    "eslint-loader": "1.4.1",
    "eslint-plugin-import": "1.10.2",
    "eslint-plugin-jsx-a11y": "1.5.3",
    "eslint-plugin-react": "5.2.2",
    "extract-text-webpack-plugin": "1.0.1",
    "fbjs": "0.8.3",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "lodash": "4.13.1",
    "material-ui": "0.15.2",
    "minimist": "1.2.0",
    "node-sass": "3.8.0",
    "react": "15.2.1",
    "react-addons-shallow-compare": "15.2.1",
    "react-dom": "15.2.1",
    "react-dropzone": "3.5.3",
    "react-tap-event-plugin": "1.0.0",
    "rimraf": "2.5.3",
    "sass-loader": "4.0.0",
    "script-loader": "0.7.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ]
  },
  "dependencies": {
    "es6-promise": "4.1.0",
    "ramda": "0.23.0"
  }
}
